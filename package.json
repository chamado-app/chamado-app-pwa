{
  "name": "chamado-app",
  "version": "0.0.1",
  "description": "An service desk application",
  "productName": "Chamado.app",
  "author": "Gabriel Scaranello <gabrieljscaranello@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint ./src --ext .vue,.ts",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .eslintignore",
    "dev": "quasar dev",
    "build": "quasar build",
    "prepare": "husky install",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests --watch",
    "test:ui": "npm run test:watch -- --ui",
    "test:ci": "npm test -- --coverage"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.5",
    "axios": "^1.2.1",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@quasar/app-vite": "^1.3.0",
    "@rushstack/eslint-patch": "^1.3.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^7.0.0",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitest/coverage-istanbul": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^36.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "git-commit-msg-linter": "^5.0.4",
    "happy-dom": "^10.3.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "prettier-config-standard": "^5.0.0",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
